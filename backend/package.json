{
  "name": "sass-backend",
  "version": "1.0.0",
  "description": "Backend for SASS starter application",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "format": "prettier --write \"src/**/*.ts\" \"*.{js,json,ts}\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"*.{js,json,ts}\""
  },
  "keywords": [
    "sass",
    "nodejs",
    "express",
    "prisma",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "google-auth-library": "^10.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.3",
    "prisma": "^6.6.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "@types/nodemailer": "^6.4.17",
    "prettier": "3.5.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
